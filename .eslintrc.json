{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "prettier/prettier": ["error"],
        "indent": ["error", 4, { "MemberExpression": 1, "SwitchCase": 1 }],
        "eol-last": "off",
        "no-use-before-define": "off",
        "no-shadow": "off",
        "operator-linebreak": "off"
    }
}
